<!doctype html>
<html>
  <head>
    <title>User Feedback Reply</title>
  </head>
  <body
    style="
      font-family: Arial, sans-serif;
      background-color: #f5f5f5;
      padding: 20px;
    "
  >
    <div
      style="
        width: 100%;
        margin: 0 auto;
        max-width: 600px;
        background-color: #fff;
      "
    >
      <div style="text-align: center; padding: 30px">
        <img
          src="https://utfs.io/f/7569a78d-1118-40e6-a844-bfc5433fcb83-yko3op.png"
          alt="GFE Logo"
          style="max-width: 150px"
        />
      </div>
      <div
        style="
          width: calc(100% - 20px);
          height: 1px;
          background-color: #eee;
          margin: 0 10px;
        "
      ></div>
      <div style="padding: 20px; margin: auto; background-color: #fff">
        <%- message %>
      </div>
    </div>
  </body>
</html>
